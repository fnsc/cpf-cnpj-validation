{
    "name": "fnsc/cpf-cnpj-validator",
    "description": "CPF and CNPJ validator for laravel applications.",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Fonseca",
            "email": "gabrieldfnsc@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "illuminate/console": "^9.9",
        "illuminate/support": "^9.9",
        "illuminate/config": "^9.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "illuminate/validation": "^9.9",
        "leroy-merlin-br/coding-standard": "v3.1.0",
        "mockery/mockery": "^1.5",
        "rector/rector": "^0.12.21",
        "phpro/grumphp": "^1.13",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Fnsc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fnsc\\": "tests/Unit/src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    }
}
